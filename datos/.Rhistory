codo.jambu= function(d){
mydata <- d
wss <- (nrow(mydata)-1)*sum(apply(mydata,2,var))
for (i in 2:15) wss[i] <- sum(kmeans(mydata, centers=i)$withinss)
plot(1:15,
wss,
type="b",
xlab="Number of Clusters",
ylab="Within groups sum of squares")
}
